{
  "dashboard": {
    "title": "Dropwizard Observability",
    "tags": ["dropwizard", "opentelemetry"],
    "timezone": "browser",
    "schemaVersion": 27,
    "version": 1,
    "refresh": "5s",
    "panels": [
      {
        "id": 1,
        "title": "Request Rate",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0},
        "targets": [
          {
            "expr": "rate(http_server_request_duration_count[5m])",
            "refId": "A"
          }
        ]
      },
      {
        "id": 2,
        "title": "Request Duration",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(http_server_request_duration_bucket[5m]))",
            "refId": "A"
          }
        ]
      },
      {
        "id": 3,
        "title": "Traces",
        "type": "traces",
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 8},
        "datasource": {
          "type": "tempo",
          "uid": "tempo"
        }
      }
    ]
  }
}

